{
  "name": "rtmi-serverless-api",
  "version": "0.1.0",
  "bin": {
    "swa-cdk-app": "bin/rtmi-serverless-api.ts"
  },
  "scripts": {
    "build": "tsc && npm prune --production",
    "build-api": "cd lambda/api && npm run build",
    "build-authorizer": "cd lambda/authorizer && npm run build",
    "build-export": "cd lambda/export && npm run build",
    "cdk": "cdk",
    "deploy": "cdk deploy --require-approval never",
    "docker-db": "cd docker; ./start-docker-db.sh",
    "docker-redis": "cd docker; ./start-docker-redis.sh",
    "lint": "eslint --ext .ts --fix .",
    "synth": "cdk synth --no-staging > template.yml",
    "test": "jest --coverage",
    "test-with-docker": "cross-env DOTENV_CONFIG_PATH=./.env.test npm run test"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "2.2.1",
    "@types/aws-lambda": "8.10.101",
    "@types/jest": "26.0.14",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cross-env": "7.0.3",
    "esbuild": "^0.14.36",
    "eslint": "^8.6.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.2.5",
    "jest": "27.1.1",
    "jest-extended": "1.2.1",
    "jest-html-reporter": "^3.1.3",
    "jest-junit": "10.0.0",
    "lint-staged": "^10.2.11",
    "source-map-support": "^0.5.21",
    "ts-jest": "27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@aws-cdk/assertions": "1.140.0",
    "@aws-cdk/aws-apigateway": "1.140.0",
    "@aws-cdk/aws-cloudformation": "1.140.0",
    "@aws-cdk/aws-ec2": "1.140.0",
    "@aws-cdk/aws-iam": "1.140.0",
    "@aws-cdk/aws-kms": "1.140.0",
    "@aws-cdk/aws-lambda": "1.140.0",
    "@aws-cdk/aws-lambda-nodejs": "1.140.0",
    "@aws-cdk/aws-logs": "1.140.0",
    "@aws-cdk/core": "1.140.0",
    "@lmig/rtmi-core-utilities": "0.12.17",
    "@lmig/swa-cdk-core": "^2.16.1",
    "@types/node": "13.7.0",
    "aws-cdk": "1.140.0",
    "aws-sdk": "^2.1077.0",
    "axios": "0.27.2",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "jose": "^4.6.0",
    "lambda-api": "^0.11.2",
    "mongodb": "4.3.1",
    "mysql2": "2.3.3",
    "redis": "4.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "repository": "https://git.forge.lmig.com/projects/rtmi/repos/rtmi-serverless-api",
  "engines": {
    "node": "14",
    "npm": "6"
  }
}

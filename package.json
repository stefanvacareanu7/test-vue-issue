  "scripts": {
    "start": "react-app-rewired start",
    "build": "yarn run build-semantic && INLINE_RUNTIME_CHUNK=false REACT_APP_VERSION_DATE=$(date '+%m/%d/%Y %T') react-app-rewired --max_old_space_size=8096 build",
    "test": "CI=true react-app-rewired --max-old-space-size=8192 --expose-gc test --coverage --runInBand --logHeapUsage --env=jsdom --transformIgnorePatterns \"node_modules/(?!react-syntax-highlighter)/\" --testPathIgnorePatterns=cypress/ ",
    "local:test": "react-app-rewired --max-old-space-size=8192 test --watchAll --env=jsdom --transformIgnorePatterns \"node_modules/(?!react-syntax-highlighter)/\" --testPathIgnorePatterns=cypress/ ",
    "local:codecov": "react-app-rewired --max-old-space-size=8192 test --watchAll --env=jsdom --coverage --transformIgnorePatterns \"node_modules/(?!react-syntax-highlighter)/\" --testPathIgnorePatterns=cypress/ ",
    "local:updateSnapshots": "react-app-rewired --max-old-space-size=8192 test -u --watchAll --env=jsdom --transformIgnorePatterns \"node_modules/(?!react-syntax-highlighter)/\" --testPathIgnorePatterns=cypress/ ",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && yarn run build-semantic && gulp watch",
    "predeploy:prod": "PUBLIC_URL=https://d2b55bwvv7wkd6.cloudfront.net yarn build",
    "deploy:prod": "aws s3 sync build s3://app.fiddler.ai",
    "postdeploy:prod": "aws s3 cp s3://app.fiddler.ai/index.html s3://app.fiddler.ai/index.html --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate",
    "predeploy:staging": "PUBLIC_URL=https://d7hiq4vcunxw4.cloudfront.net yarn build",
    "deploy:staging": "aws s3 sync build s3://staging.fiddler.ai",
    "postdeploy:staging": "aws s3 cp s3://staging.fiddler.ai/index.html s3://staging.fiddler.ai/index.html --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate",
    "predeploy:test": "PUBLIC_URL=https://d2ksf2suconcdp.cloudfront.net yarn build",
    "deploy:test": "aws s3 sync build s3://test.fiddler.ai",
    "postdeploy:test": "aws s3 cp s3://test.fiddler.ai/index.html s3://test.fiddler.ai/index.html --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate"
